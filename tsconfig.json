{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020"
	}
}
